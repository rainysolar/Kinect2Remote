<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="ipAddress" value="127.0.0.1"/>
    <add key="exchange" value="KinectData"/>
    <!-- 
      A senderID is a (possibly) unique identifier to indice which remote
      is sending the data, so that a receiver which cares only about a few
      of them can filter them out.
    -->
    <add key="senderID" value="defaultRemote"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>